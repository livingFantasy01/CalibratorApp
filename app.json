{
  "expo": {
    "name": "CalibratorApp",
    "slug": "CalibratorApp",
    "newArchEnabled": true,
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "calibratorapp",
    "userInterfaceStyle": "dark",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.play.calibratorapp"
    },
    "android": {
      "package": "com.play.calibratorapp",
      "versionCode": 1,
      "jsEngine": "hermes",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "HIGH_SAMPLING_RATE_SENSORS",
        "INTERNET",
        "com.google.android.gms.permission.AD_ID"
      ],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-7403534919707613~5471818790"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#050608"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow CalibratorApp to use your location for accurate compass alignment."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#050608"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-7403534919707613~5471818790",
          "iosAppId": "ca-app-pub-7403534919707613~5471818790",
          "delayAppMeasurementInit": false
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "kotlinVersion": "2.0.0",
            "enableShrinkResourcesInReleaseBuilds": true,
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep public class com.horcrux.svg.** { *; }",
            "buildArchs": ["arm64-v8a"]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "71a1c3f3-3615-42f7-a8cb-817aed74c1bb"
      }
    }
  }
}